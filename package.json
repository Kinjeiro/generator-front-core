{
  "name": "generator-front-core",
  "version": "1.0.29",
  "description": "",
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "test": "jest",
    "--postversion": "git push --follow-tags --no-verify && npm publish"
  },
  "husky": {
    "hooks": {
      "--pre-push": "npm version patch -m \"chore(*) patch version: %s\"",
      "post-commit": "npm version patch -m \"chore(*) patch version: %s\" && git push --follow-tags --no-verify && npm publish"
    }
  },
  "commentDependecies": {
    "gulp-rename": "~1.4.0 - https://github.com/yeoman/yo/issues/577#issuecomment-392325341 - Error [ERR_STREAM_WRITE_AFTER_END]: write after end"
  },
  "dependencies": {
    "chalk": "~2.1.0",
    "gulp-rename": "1.2.2",
    "lodash": "~4.17.11",
    "yeoman-generator": "~2.0.1",
    "yosay": "~2.0.1"
  },
  "devDependencies": {
    "eslint": "~4.1.0",
    "eslint-config-prettier": "~2.4.0",
    "eslint-config-xo": "~0.18.2",
    "eslint-plugin-prettier": "~2.2.0",
    "husky": "~1.0.1",
    "jest": "~21.0.0",
    "jest-cli": "~21.0.0",
    "lint-staged": "~4.2.1",
    "nsp": "~2.6.3",
    "prettier": "~1.7.0",
    "yeoman-assert": "~3.1.0",
    "yeoman-test": "~1.7.0"
  },
  "homepage": "",
  "author": {
    "name": "Andrey Kuzmin",
    "email": "kinjeiro@gmail.com",
    "url": ""
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "",
    "yeoman-generator"
  ],
  "jest": {
    "testEnvironment": "node"
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "Kinjeiro/generator-front-core",
  "license": "Apache-2.0"
}
